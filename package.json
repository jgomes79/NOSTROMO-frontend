{
  "name": "nostromo-front",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "npm run dev",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "lint:deps": "npx depcruise src",
    "types:check": "tsc --noEmit",
    "git:pre-commit": "npm run types:check && npm run lint:deps && lint-staged",
    "test:base": "vitest --passWithNoTests",
    "test": "npm run test:coverage",
    "test:ui": "npm run test:base -- --ui",
    "test:run": "npm run test:base -- --run",
    "test:coverage": "npm run test:base -- --run --coverage",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "lint-staged": {
    "(src)/**/*.(ts|tsx)": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@metamask/providers": "^18.1.2",
    "@qubic-lib/qubic-ts-library": "^0.1.5",
    "@qubic-lib/qubic-ts-vault-library": "^1.0.2",
    "@tanstack/react-query": "5.40.1",
    "@walletconnect/sign-client": "^2.19.2",
    "axios": "1.7.2",
    "base-64": "^1.0.0",
    "buffer": "^6.0.3",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "deepmerge-ts": "7.0.2",
    "framer-motion": "11.12.0",
    "i18next": "23.11.5",
    "i18next-browser-languagedetector": "8.0.0",
    "intersection-observer": "0.12.2",
    "js-cookie": "3.0.5",
    "query-string": "9.0.0",
    "react": "18.3.1",
    "react-datepicker": "^7.6.0",
    "react-dom": "18.3.1",
    "react-dropzone": "14.3.5",
    "react-hook-form": "7.53.2",
    "react-i18next": "14.1.2",
    "react-icons": "5.3.0",
    "react-router-dom": "6.23.1",
    "react-use": "17.5.1",
    "zod": "3.23.8",
    "zustand": "4.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@juggle/resize-observer": "3.4.0",
    "@tanstack/eslint-plugin-query": "5.35.6",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "16.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/base-64": "^1.0.2",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.12.0",
    "@typescript-eslint/parser": "7.12.0",
    "@vitejs/plugin-react-swc": "3.7.0",
    "@vitest/coverage-v8": "1.6.0",
    "concurrently": "9.1.2",
    "dependency-cruiser": "16.3.2",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-refresh": "0.4.7",
    "husky": "9.0.11",
    "install": "0.13.0",
    "jsdom": "24.1.0",
    "lint-staged": "15.2.5",
    "matchmedia-polyfill": "0.3.2",
    "react-remove-attr": "0.0.6",
    "sass": "1.55.0",
    "typescript": "5.4.5",
    "vite": "5.2.12",
    "vite-plugin-checker": "0.6.4",
    "vite-plugin-svgr": "4.3.0",
    "vitest": "1.6.0",
    "vitest-sonar-reporter": "2.0.0"
  }
}
